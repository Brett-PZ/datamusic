/* name this database: sockmarket */

CREATE TABLE music3 (
    id INTEGER PRIMARY KEY AUTO_INCREMENT,
    name TEXT(20),
    song TEXT(50),
    artist TEXT(50),
    mood TEXT,
    lastTimeYouListened DATE,
    link TEXT
    );

